	$(MAKE) install_test DATADIR=$(DATADIR)/mlkit
	$(MAKE) install_include DATADIR=$(DATADIR)/mlkit
	$(MKDIR) $(DATADIR)/doc
	$(MKDIR) $(DATADIR)/doc/mlkit
	$(MAKE) install_license KIND=mlkit
	$(INSTALLDATA) doc/mlkit.pdf $(DATADIR)/doc/mlkit/
